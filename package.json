{
  "name": "nestjs-permissions",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "pm2-runtime start dist/main.js",
    "migration:create:initial": "mikro-orm migration:create --initial",
    "migration:create": "mikro-orm migration:create",
    "migration:up": "mikro-orm migration:up",
    "migration:down": "mikro-orm migration:down",
    "migration:fresh": "mikro-orm migration:fresh --seed",
    "seeder:create": "mikro-orm seeder:create",
    "seeder:run": "mikro-orm seeder:run",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@mikro-orm/cli": "^5.7.14",
    "@mikro-orm/core": "^5.7.14",
    "@mikro-orm/migrations": "^5.7.14",
    "@mikro-orm/nestjs": "^5.2.1",
    "@mikro-orm/postgresql": "^5.7.14",
    "@mikro-orm/reflection": "^5.7.14",
    "@mikro-orm/seeder": "^5.7.14",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs/common": "^10.2.2",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.2.2",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/microservices": "^10.2.2",
    "@nestjs/platform-express": "^10.2.2",
    "@nestjs/platform-socket.io": "^10.2.2",
    "@nestjs/swagger": "^7.1.9",
    "@nestjs/websockets": "^10.2.2",
    "@willsoto/nestjs-prometheus": "^5.3.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "nestjs-pino": "^3.4.0",
    "nestjs-zod": "^2.3.3",
    "pino-pretty": "^10.2.0",
    "pm2": "^5.3.0",
    "prom-client": "^14.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "ts-pattern": "^5.0.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.16",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.80",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.2",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1",
      "^@aggregates/(.*)$": "<rootDir>/../src/core/aggregates/$1"
    }
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/db/mikro-orm.config.ts",
      "./dist/db/mikro-orm.config.js"
    ]
  }
}
